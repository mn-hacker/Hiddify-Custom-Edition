{
	{% if hconfigs['naive_enable'] %}
		"inbounds": [
			{% for d in domains if d['internal_port_naive']  %}
			{%set domain=d['domain']%}
			{%set port=d['internal_port_naive']%}

			{
				"type": "http",
				"tag": "naive_in_{{port}}",
				"listen": "::",
				"listen_port": {{ port }},
                "sniff": true,
                "domain_strategy": "prefer_ipv4",
				"users": [
					{% for u in users %}
						{"username": "{{ u['uuid'] }}", "password": "{{ u['uuid'] }}"},
					{% endfor%}
				],
				"tls": {
					"enabled": true,
					"server_name": "{{domain}}",
                    "alpn": ["h2", "http/1.1"],
					"min_version": "1.2",
					"max_version": "1.3",
					{% if exec("[ -f /opt/hiddify-manager/ssl/"+domain[0:64]+".crt ]&& echo -n 'true' || echo -n 'false'")=='true' %}
					"certificate_path": "/opt/hiddify-manager/ssl/{{domain[0:64]}}.crt",
					"key_path": "/opt/hiddify-manager/ssl/{{domain[0:64]}}.crt.key",
					{%else %}
					{%set cert=exec("ls /opt/hiddify-manager/ssl/*.crt | tail -1").strip()%}
					"certificate_path": "{{cert}}",
					"key_path": "{{cert}}.key",
					{%endif%}
				}
                {% if hconfigs['naive_padding'] %}
                ,"padding": true
                {% endif %}
			},
			{%endfor%}
		]
	{% endif %}
}
