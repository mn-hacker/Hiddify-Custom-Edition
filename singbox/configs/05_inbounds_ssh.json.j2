{
    "inbounds":[
        {% if  hconfigs['ssh_server_enable'] %}
                {
                    "tag":"v10-ssh",
                    "listen": "0.0.0.0",
                    "listen_port":{{ hconfigs['ssh_server_port'] }},
                    "tcp_fast_open": true,
                    "sniff": true,
                    "sniff_override_destination": true,
                    "domain_strategy": "prefer_ipv4",
                    
                    "host_key":[
                        "{{hconfigs['ssh_host_rsa_pk'].replace("\n","\\n")}}",
                        "{{hconfigs['ssh_host_ecdsa_pk'].replace("\n","\\n")}}",
                        "{{hconfigs['ssh_host_ed25519_pk'].replace("\n","\\n")}}"
                    ],
                    {% include '/opt/hiddify-manager/singbox/configs/common/protocols/ssh.pj2' %},
                }
                
        {%endif%}
    ]
}
