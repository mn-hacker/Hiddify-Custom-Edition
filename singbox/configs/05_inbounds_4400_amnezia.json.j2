{
	{% if hconfigs['amnezia_enable'] %}
		"inbounds": [
			{% for d in domains if d['internal_port_amnezia']  %}
			{%set domain=d['domain']%}
			{%set port=d['internal_port_amnezia']%}

			{
				"type": "amneziawg",
				"tag": "amnezia_in_{{port}}",
				"listen": "::",
				"listen_port": {{ port }},
				"sniff": true,
				"sniff_override_destination": true,
				"domain_strategy": "prefer_ipv4",
				"users": [
					{% for u in users %}
						{
							"name": "{{ u['uuid'] }}@hiddify.com",
							"private_key": "{{ u['wg_pk'] }}",
							"client_ip": ["{{ u['wg_ipv4'] }}/32", "{{ u['wg_ipv6'] }}/128"]
						},
					{% endfor%}
				],
				"local_address": ["10.111.0.1/24", "fc00::1/64"],
				"mtu": 1380,
				"packet_encoding": "xudp",
				"server_private_key": "{{ hconfigs['wg_private_key'] }}",
				"s1": {{ hconfigs['amnezia_s1'] }},
				"s2": {{ hconfigs['amnezia_s2'] }},
				"h1": {{ hconfigs['amnezia_h1'] }},
				"h2": {{ hconfigs['amnezia_h2'] }},
				"h3": {{ hconfigs['amnezia_h3'] }},
				"h4": {{ hconfigs['amnezia_h4'] }},
				"jc": {{ hconfigs['amnezia_jc'] }},
				"jmin": {{ hconfigs['amnezia_jmin'] }},
				"jmax": {{ hconfigs['amnezia_jmax'] }}
			},
			{%endfor%}
		]
	{% endif %}
}
